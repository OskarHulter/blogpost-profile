    "geist": "1.3.1",
    "next": "15.0.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "swr": "2.2.5",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/eslint": "8.56.12",
    "@types/node": "20.17.9",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
